    <Version>0.1.0-alpha</Version>
    <Authors>Arturo Torres</Authors>
    <PackageId>ArturoTorres.QApp</PackageId>
    <Company>Arturo Torres</Company>
    <Copyright>Copyright Â©2018 ArturoTorres</Copyright>
    <PackageLicenseUrl>https://raw.githubusercontent.com/arttorres/QApp/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/arttorres/QApp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/arttorres/QApp</RepositoryUrl>
    <PackageTags>console framework quick app</PackageTags>
    <Description>A light console framework for quick applications.</Description>
