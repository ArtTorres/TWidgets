<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.0;netstandard2.0;net40;net45</TargetFrameworks>
    <Version>0.3.0-alpha</Version>
    <Authors>Arturo Torres</Authors>
    <PackageId>TWidgets</PackageId>
    <Company>Arturo Torres</Company>
    <Copyright>Copyright Â©2019 ArturoTorres</Copyright>
    <PackageLicenseUrl>https://raw.githubusercontent.com/arttorres/TWidgets/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/arttorres/TWidgets</PackageProjectUrl>
    <RepositoryUrl>https://github.com/arttorres/TWidgets</RepositoryUrl>
    <PackageTags>console framework widgets</PackageTags>
    <Description>A Text-based widget framework for console applications.</Description>
    <RepositoryType>Git</RepositoryType>
    <PackageReleaseNotes>Input Widgets support.
Added ProgressBar widget.
Added ProgressChar widget.
Added TextInput widget.
Minor improvements.</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <SignAssembly>false</SignAssembly>
    <PackageIconUrl></PackageIconUrl>
    <AssemblyVersion>0.3.0.0</AssemblyVersion>
    <FileVersion>0.3.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Widgets\ProgressList.cs" />
    <Compile Remove="Widgets\SelectInput.cs" />
  </ItemGroup>

</Project>
